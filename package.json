{
  "name": "serverless-docker-typescript-template",
  "version": "1.0.0",
  "description": "A serverless docker express template",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/app.ts",
    "start": "node dist/app.js",
    "build": "tsc && tsc-alias && cpy \"public/**/*\" dist/public",
    "test": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "16.4.5",
    "express": "4.18.3",
    "joi": "18.0.0",
    "loggerfy": "1.1.4",
    "zod": "4.0.17"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jest": "30.0.0",
    "@types/node": "20.11.25",
    "cpy-cli": "5.0.0",
    "jest": "30.0.5",
    "nodemon": "3.1.0",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "tsc-alias": "1.8.16",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.2"
  }
}
